<?php
include "DB.php";



$columns = implode(', ', array_keys($entries));
$values = implode(', ', array_fill(0, count($entries), '?'));

$sql = "INSERT INTO form2 ($columns) VALUES ($values)";
$stmt = $conn->prepare($sql);

// Bind parameters
$types = str_repeat('s', count($entries));  // Assuming all values are strings
$stmt->bind_param($types, ...array_values($entries));

// Execute the statement
if ($stmt->execute()) {
    echo "Record inserted successfully";
} else {
    echo "Error: " . $stmt->error;
}

// Close the statement
$stmt->close();
?>